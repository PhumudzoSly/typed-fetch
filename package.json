{
  "name": "@phumu/typed-fetch",
  "version": "0.0.1",
  "description": "Privacy-first status-aware typed fetch with type generation from response shapes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "typed-fetch": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "generate": "node dist/cli.js generate",
    "check": "node dist/cli.js check",
    "clean": "node dist/cli.js clean",
    "listen": "node dist/cli.js listen",
    "test": "npm run build && node --test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PhumudzoSly/typed-fetch"
  },
  "keywords": [
    "fetch",
    "typescript",
    "type-safe",
    "http",
    "typegen",
    "privacy",
    "status-aware"
  ],
  "author": "Phumudzo",
  "license": "MIT",
  "dependencies": {
    "boxen": "^8.0.1",
    "commander": "^14.0.1",
    "figlet": "^1.8.2",
    "node-fetch": "^2.x",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.13.14",
    "@types/node-fetch": "^2.x",
    "typescript": "^5.8.2"
  }
}
